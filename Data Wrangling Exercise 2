read.csv(file = "titanic_original.csv", header = FALSE)

class(data)
dim(data)
titanic_clean <- read.csv(file = "titanic_original.csv")
head (titanic_clean)

titanic_clean <- read.csv("titanic_original.csv", header=T, na.strings=c(""))
titanic_clean$boat<-replace(titanic_clean$boat,titanic_clean$boat==1,"NA" )
head(titanic_clean)

titanic_clean %>% 
  summarise(avg_age = mean(age, na.rm = TRUE))

titanic_clean <- titanic_clean %>% 
  mutate(has_cabin_number = ifelse(cabin == "", 0, 1))
head(titanic_clean)

write.csv(titanic_clean, file = "titanic_clean.csv")
